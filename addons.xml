<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.daddylive" name="DaddyLive v3" version="1.2.2" provider-name="Guenole84">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
        <import addon="script.module.requests" />
        <import addon="script.module.inputstreamhelper" />
        <import addon="inputstream.ffmpegdirect" />
        <import addon="script.module.six"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Live TV and Sports</summary>
        <description lang="en">Live TV and Sports</description>
        <platform>all</platform>
        <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
        <news>v1.2.2: Fix crash Kodi 19+ (xbmcvfs.translatePath au lieu de xbmc.translatePath)</news>
    </extension>
</addon>
<addon id="repository.gwen84" name="Gwen84 Repository" version="1.0.0" provider-name="Guenole84">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="Gwen84 Repository">
        <dir>
            <info compressed="false">https://guenole84.github.io/addons.xml</info>
            <checksum>https://guenole84.github.io/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Guenole84/guenole84.github.io/main/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Gwen84 Repository</summary>
        <description lang="en">Gwen84 Kodi addon repository</description>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
        </assets>
    </extension>
</addon>
</addons>
